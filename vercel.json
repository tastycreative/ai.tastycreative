{
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 60
    },
    "app/api/upload/**/*.ts": {
      "maxDuration": 60,
      "memory": 1024
    },
    "app/api/webhooks/**/*.ts": {
      "maxDuration": 300
    },
    "app/api/training/**/*.ts": {
      "maxDuration": 300
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}
